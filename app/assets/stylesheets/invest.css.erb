#one {

  background-image: url('<%= asset_path("index/invest.jpg") %>');
  background-size: 100% 100%;
  background-position: top;
  background-attachment: scroll;
  background-repeat: no-repeat;
  text-align: center;
  /*border-top: 0.5em solid #5385c1;*/
}



//md
@media screen and (min-width:768px){
  #one p {
    color: #409FB2;
    font-weight: 800;
    font-size: 36px;
  }
  #one h2 {
    letter-spacing: 1px;
    font-size: 20px;
    font-weight: 400;
    text-align: center;
    line-height: 1.2em;
  }
  #one .header {
    padding-bottom: 4em;
  }

  #one .number {
    padding-bottom: 2em;
  }
}

//xs
@media screen and (max-width:767px){
  #one p {
    color: #409FB2;
    font-weight: 700;
    font-size: 20px;

  }
  #one h2 {
    letter-spacing: 1px;
    font-size: 1.5em;
    font-weight: 300;
    text-align:center;
  }
  #one .header {
    padding-bottom: 1em;
  }
}

#two {

